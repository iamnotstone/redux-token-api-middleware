{
  "name": "redux-token-api-middleware",
  "version": "0.8.4",
  "description": "Redux middleware for calling APIs with token-based auth",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "watch 'npm run build' ./src/",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive ./test",
    "start": "nodemon --watch examples/ --ignore examples/*.xml  examples/server.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "javascript",
    "redux",
    "middleware",
    "api",
    "token",
    "jwt"
  ],
  "author": "Edward Stone <edwardjstone@yahoo.com>",
  "repository": "https://github.com/eadmundo/redux-token-api-middleware",
  "bugs": {
    "url": "https://github.com/eadmundo/redux-token-api-middleware/issues"
  },
  "homepage": "https://github.com/eadmundo/redux-token-api-middleware",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^5.7.0",
    "lodash.isarraylikeobject": "^4.2.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isundefined": "^3.0.1",
    "lodash.map": "^4.6.0",
    "lodash.omitby": "^4.6.0",
    "lodash.startswith": "^4.2.1",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.2",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "eslint": "~2.2.0",
    "express": "^4.16.2",
    "express-urlrewrite": "^1.2.0",
    "html-webpack-plugin": "^2.14.0",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.5.2",
    "watch": "^0.17.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-middleware": "^2.0.6"
  },
  "npmName": "redux-api-token-middleware",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
